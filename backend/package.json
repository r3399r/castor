{
  "name": "caster-backend",
  "version": "0.0.1",
  "scripts": {
    "lint:es": "eslint src",
    "fix:es": "npm run lint:es -- --fix",
    "lint:format": "prettier --config ./prettierrc.json --list-different \"./**/*.ts\" --ignore-path .gitignore",
    "fix:format": "npm run lint:format -- --write",
    "lint": "run-p lint:*",
    "fix": "run-s fix:*",
    "clean": "del-cli dist lib -f",
    "build:layer": "npm install --production && mkdir -p dist/nodejs && cp -R node_modules dist/nodejs",
    "build:lambda": "npm install && tspc",
    "pre:deploy": "run-s clean build:*"
  },
  "author": "Yue Chang",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "inversify": "^7.10.1",
    "reflect-metadata": "^0.2.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "del-cli": "^7.0.0",
    "eslint": "^9.36.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "typescript": "^5.9.2",
    "typescript-transform-paths": "^3.5.5"
  }
}
